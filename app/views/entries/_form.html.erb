<%= form_for(@entry) do |f| %>

  <%#= f.select :user_id, User.all.collect{|e| [e.email, e.id]} %>
  <%= f.label 'Positive Karma' %>
 
  <%= f.text_field :pos_karma %>

  <%= f.label 'Negative Karma' %>
  <%= f.text_field :neg_karma %>

  <%= f.label 'Note' %>
  <%= f.text_field :note %>

  <br>
  <%= f.label 'Mood Rating' %>
  <%= radio_button("entry", "rating", 1) %> 1
  <%= radio_button("entry", "rating", 2) %> 2
  <%= radio_button("entry", "rating", 3) %> 3
  <%= radio_button("entry", "rating", 4) %> 4
  <%= radio_button("entry", "rating", 5) %> 5
  <br>
  <br>
  <%= f.submit 'save', :class => 'btn' %>
<% end %>